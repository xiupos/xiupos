{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": "./",
    "strictNullChecks": true,
    "allowJs": true,
    "checkJs": true,
    "paths": {
      "@components/*": ["src/components/*"],
      "@content/*": ["src/content/*"],
      "@plugins/*": ["src/plugins/*"],
      "@assets/*": ["src/assets/*"]
    }
  }
}
